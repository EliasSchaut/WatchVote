{
  "name": "movie-monday-manager",
  "version": "1.0.0-alpha.8",
  "description": "A website for adding and voting movies for the next watch party",
  "author": "EliasSchaut",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:client": "npm i --prefix ./client && npm run build --prefix ./client",
    "build:db": "prisma migrate dev --schema=./prisma/schema.prisma",
    "build:docs": "npx @compodoc/compodoc -p tsconfig.json -n \"MoMoMat Backend Docs\" -d docs",
    "build:all": "npm run build:client && npm i && npm run build:db && npm run build:docs && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docs": "npx @compodoc/compodoc -s -d docs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "cron-parser": "^4.7.1",
    "crypto-js": "^4.1.1",
    "cuid": "^2.1.8",
    "discord.js": "^14.7.1",
    "helmet": "^5.1.1",
    "imdb-api": "^4.4.1",
    "joi": "^17.7.0",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^4.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.16",
    "@types/jest": "28.1.4",
    "@types/node": "^16.18.11",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
