{
  "name": "watchvote",
  "version": "1.0.0-alpha.8",
  "description": "A website for adding and voting movies for the next watch party",
  "author": "EliasSchaut",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EliasSchaut/WatchVote"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:client": "npm i --prefix ./client && npm run build --prefix ./client",
    "build:db": "prisma migrate dev --schema=./prisma/schema.prisma",
    "build:docs": "npx @compodoc/compodoc -p tsconfig.json -n \"MoMoMat Backend Docs\" -d docs",
    "build:all": "npm run build:client && npm i && npm run build:db && npm run build:docs && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docs": "npx @compodoc/compodoc -s -d docs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.1",
    "cron-parser": "^4.9.0",
    "crypto-js": "^4.1.1",
    "cuid": "^2.1.8",
    "discord.js": "^14.13.0",
    "helmet": "^5.1.1",
    "imdb-api": "^4.4.1",
    "joi": "^17.9.2",
    "nestjs-i18n": "^10.3.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.4.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.4",
    "@types/node": "^16.18.42",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
